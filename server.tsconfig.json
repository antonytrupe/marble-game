{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "useDefineForClassFields": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "outDir":"dist/server"
  },
  "include": [
    "src/server"
  ]
}